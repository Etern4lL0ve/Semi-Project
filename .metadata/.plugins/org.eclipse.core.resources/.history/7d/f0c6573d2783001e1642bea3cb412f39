<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>

<entry key="insertChatMessage">
    INSERT INTO CHAT (CHAT_NAME, CHAT_CONTENT, CHAT_DATE)
    VALUES (?, ?, SYSDATE)
</entry>


<entry key="selectChatMessages">
    SELECT CHAT_NAME, CHAT_CONTENT, CHAT_DATE
    FROM CHAT
    ORDER BY CHAT_DATE
</entry>


</properties>